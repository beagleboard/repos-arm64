#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

override_dh_auto_configure:
	git -c http.sslVerify=false clone https://git.beagleboard.org/RobertCNelson/apindex-v2.git --depth=1
	cd ./apindex-v2/ ; cmake . -DCMAKE_INSTALL_PREFIX=./tmp ; make install

%:
	dh $@
